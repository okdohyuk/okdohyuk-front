{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "~": ["./"],
      "~/*": ["./*"],
      "@components": ["./components"],
      "@components/*": ["./components/*"],
      "@hooks": ["./hooks"],
      "@hooks/*": ["./hooks/*"],
      "@utils": ["./utils"],
      "@utils/*": ["./utils/*"],
      "@stores": ["./stores"],
      "@stores/*": ["./stores/*"],
      "@libs": ["./libs"],
      "@libs/*": ["./libs/*"],
      "@assets": ["./assets"],
      "@assets/*": ["./assets/*"],
      "@api": ["./spec/api"],
      "@api/*": ["./spec/api/*"],
      "@queries": ["./queries"],
      "@queries/*": ["./queries/*"],
      "@context": ["./context"],
      "@context/*": ["./context/*"]
    }
  }
}
